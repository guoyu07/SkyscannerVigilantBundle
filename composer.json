{
  "name": "jeancsil/skyscanner-vigilant-bundle",
  "type": "symfony-bundle",
  "description": "Provides constant tracking for the best prices for your next trip",
  "keywords": [
    "Skyscanner",
    "Flights",
    "Travel",
    "Trip"
  ],
  "homepage": "https://github.com/jeancsil/SkyscannerVigilantBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Jean Silva",
      "email": "jeancsil@gmail.com",
      "homepage": "https://github.com/jeancsil"
    }
  ],
  "require": {
    "php": ">=5.5",
    "symfony/symfony": "3.1.*",
    "symfony/class-loader": "^2.3 || ^3.0",
    "symfony/config": "^2.3.9 || ^3.0",
    "symfony/console": "^2.3 || ^3.0",
    "symfony/dependency-injection": "^2.3.3 || ^3.0",
    "symfony/http-foundation": "^2.3 || ^3.0",
    "symfony/property-access": "^2.3 || ^3.0",
    "symfony/validator": "^2.3 || ^3.0",
    "guzzlehttp/guzzle": "6.2.*"
  },
  "autoload": {
    "psr-4": {
      "Jeancsil\\Skyscanner\\VigilantBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "config": {
    "sort-packages": true
  }
}